{
  "version": 2,
  "name": "bettr-fit",
  "alias": "bettr.fit",
  "build": {
    "env": {
      "REACT_APP_API_ENDPOINT": "https://bettr-fit-api.herokuapp.com/api",
      "REACT_APP_TOKEN_KEY": "bettr-fit-auth-token"
    }
  },
  "routes": [
      {
        "src": "^/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": ".*",
        "dest": "/index.html"
      },
      {
        "src": "^/manifest.json$",
        "dest": "/manifest.json"
      }
  ]
}